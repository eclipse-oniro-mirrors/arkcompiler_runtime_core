        if (fromIndex >= this.length as int) {
            fromIndex = this.length as int - 1
        }
        if (fromIndex < 0) {
            fromIndex = this.length as int + fromIndex
        }

        for (let i = fromIndex; i >= 0; --i) {
            let tmp = this.at(i)
            if (tmp == undefined) {
                return -1
            }
            if (tmp.intValue() == val) {
                return i
            }
        }
        return -1
    }

