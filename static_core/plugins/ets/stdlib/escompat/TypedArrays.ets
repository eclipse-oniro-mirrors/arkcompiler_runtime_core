        let res: StringBuilder = new StringBuilder("")
        for (let i = 0; i < this.length as int - 1; ++i) {
            res.append(this.getUnsafe(i) as number)
            res.append(s)
        }
        if (this.length as int > 0) {
            res.append(this.getUnsafe(this.length as int - 1) as number)
        }
        return res.toString()
    }

