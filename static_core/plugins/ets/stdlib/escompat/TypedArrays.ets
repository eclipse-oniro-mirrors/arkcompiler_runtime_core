     *
     * @link https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/TypedArray/lastIndexOf
     */
    public lastIndexOf(val: byte, fromIndex: int): int {
        if (fromIndex >= this.length as int) {
