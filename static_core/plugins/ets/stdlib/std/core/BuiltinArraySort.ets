    if (j + 1 == endIndex) {
        while (i < j && !((pivot.compareTo(arr[i])) < 0)) {
            i++
        }
    } else {
        while (!((pivot.compareTo(arr[i])) < 0)) {
            i++
        }
    }
    while (i < j) {
        // Here ((pivot.compareTo(arr[i])) < 0) and !((pivot.compareTo(arr[j])) < 0) holds
        let tmp = arr[i]
        arr[i] = arr[j]
        arr[j] = tmp
        while (!((pivot.compareTo(arr[++i])) < 0)) {}
        while (((pivot.compareTo(arr[--j])) < 0)) {}
