TEST_F(FunctionCall{{ Type }}Test, cross_thread_function_call_from_native_function)
{
    std::array functions = {
        ani_native_function {"foo", ":", reinterpret_cast<void *>(A::NativeFoo)},
        ani_native_function {"baz", ":", reinterpret_cast<void *>(A::NativeBaz<FunctionCall{{ Type }}Test>)}};
    ASSERT_EQ(env_->Module_BindNativeFunctions(module_, functions.data(), functions.size()), ANI_OK);

    std::thread([this]() {
        ani_env *env {};
        ASSERT_EQ(vm_->AttachCurrentThread(nullptr, ANI_VERSION_1, &env), ANI_OK);

        ani_module module {};
        ASSERT_EQ(env->FindModule("@functionModule.ani_verify_test_function_call_{{ type }}", &module), ANI_OK);
        ani_function function {};
        ASSERT_EQ(env->Module_FindFunction(module, "foo", ":", &function), ANI_OK);

        // In the native "foo" function, the "voidParamFunction" function is found
        ASSERT_EQ(env->Function_Call_Void(function), ANI_OK);

        ASSERT_EQ(vm_->DetachCurrentThread(), ANI_OK);
    }).join();

    ani_function function {};
    ASSERT_EQ(env_->Module_FindFunction(module_, "baz", ":", &function), ANI_OK);
    // In the native "baz" function, the "voidParamFunction" function is called
    ASSERT_EQ(env_->Function_Call_Void(function), ANI_OK);
}

