class A {
public:
    static ani_function g_function;

    static void NativeFoo(ani_env *env, ani_object)
    {
        ani_module module {};
        ASSERT_EQ(env->FindModule("@functionModule.ani_verify_test_function_call_{{ type }}", &module), ANI_OK);

        ASSERT_EQ(env->Module_FindFunction(module, "voidParamFunction", ":{{ ret }}", &g_function), ANI_OK);
    }

    template <typename TestType>
    static void NativeBaz(ani_env *env, ani_object)
    {
        ani_{{ type }} result;
        if constexpr (std::is_same_v<TestType, FunctionCall{{ Type }}ATest>) {
            ani_value args[2U] = {};
            ASSERT_EQ(env->c_api->Function_Call_{{ Type }}_A(env, g_function, &result, args), ANI_OK);
        } else {
            ASSERT_EQ(env->c_api->Function_Call_{{ Type }}(env, g_function, &result), ANI_OK);
        }
    }

    template <typename TestType>
    static void NativeBar(ani_env *env, ani_object, ani_long m)
    {
        ani_function function = reinterpret_cast<ani_function>(m);

        ani_{{ type }} result;
        if constexpr (std::is_same_v<TestType, FunctionCall{{ Type }}ATest>) {
            ani_value args[2U] = {};
            ASSERT_EQ(env->c_api->Function_Call_{{ Type }}_A(env, function, &result, args), ANI_OK);
        } else {
            ASSERT_EQ(env->c_api->Function_Call_{{ Type }}(env, function, &result), ANI_OK);
        }
    }
};

ani_function A::g_function = nullptr;

