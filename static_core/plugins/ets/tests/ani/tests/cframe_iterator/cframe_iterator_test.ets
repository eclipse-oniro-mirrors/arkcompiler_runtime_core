class Test {
    public native genericMethod(a: int, b: long, c: boolean): boolean;
    public static native staticMethod(a: int, b: long, c: boolean): boolean;

    @ani.unsafe.Quick
    public static native quickMethod(a: int, b: long, c: boolean): boolean;
};

function callGenericMethod(a: int, b: long, c: boolean) {
    let test = new Test();
    return test.genericMethod(a, b, c);
}

function callStaticMethod(a: int, b: long, c: boolean) {
    return Test.staticMethod(a, b, c);
}

function callQuickMethod(a: int, b: long, c: boolean) {
    return Test.quickMethod(a, b, c);
}
