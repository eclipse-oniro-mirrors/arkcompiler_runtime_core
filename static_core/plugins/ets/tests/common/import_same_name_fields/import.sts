import * as A from "./class_module.sts"

class ThisFile {
    static b: int = 56;
    b: int = 32;
}

class ThisDiff {
    static c: string = "s";
    c: int = 6;
}

function main() : void {
    assert A.BothInt.a == 24;
    let bothInst = new A.BothInt;
    assert bothInst.a == 42;
    assert A.IntAndString.a == 23;
    let inst = new A.IntAndString;
    assert inst.a == "42";
    assert ThisFile.b == 56;
    let fileInst = new ThisFile;
    assert fileInst.b == 32;
    assert ThisDiff.c == "s";
    let diffInst = new ThisDiff;
    assert diffInst.c == 6;
}
