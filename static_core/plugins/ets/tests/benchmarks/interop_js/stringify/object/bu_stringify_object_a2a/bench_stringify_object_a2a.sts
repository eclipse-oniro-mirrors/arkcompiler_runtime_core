class Person {
    name = 'string';
    constructor() {}
}

export class StringifyObjectA2a {

    state: number = 0
    totalTime: number = 0;
    runsLeft: number = 0;

    public setup(): void {
        console.log('starting')
    }

    test(): number {
        this.setup();
        const obj = new Person();

        let iterations: number = this.runsLeft;
        let start: number;
        let loopTime: number = 0;

        while (iterations > 0) {
            start = Chrono.nanoNow();
            JSON.stringify(obj);

            loopTime += Chrono.nanoNow() - start;
            iterations = iterations - 1;
        }

        console.log(`Benchmark result: stringify_object_a2a ` + loopTime);
        this.totalTime += loopTime;

        return this.totalTime;
    }
}
