import { binaryStringToNumberToString } from 'test_import';

export class ConversionBinaryA2j {

    totalTime: number = 0;
    runsLeft: number = 10000;
    binaryString: string = '';
    result: string = '';
    seed: number = 123;

    generateNumber(seed: number): number {
        const modulus = Math.pow(2, 32);
        const a = 1664525;
        const c = 1013904223;
      
        seed = (a * seed + c) % modulus;
        
        return Math.floor((seed / modulus) * 100); 
      }
   
    public setup(): void {
        const data: Number = this.generateNumber(this.seed);
        this.binaryString = data.toString(2);
    }

    test(): number {
        this.setup();
        while (this.runsLeft > 0) {
            const start = Chrono.nanoNow();
            this.result = binaryStringToNumberToString(this.binaryString)
            const end = Chrono.nanoNow();
            this.runsLeft = this.runsLeft - 1;
            this.totalTime = this.totalTime + (end - start);
        }
        return this.totalTime;
    }
}
