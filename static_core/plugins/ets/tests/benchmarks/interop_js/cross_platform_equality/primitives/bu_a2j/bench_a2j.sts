import {
    jsBigInt,
    jsBool,
    jsNull,
    jsUndefined,
} from 'import';

const MS2NS = 1000000;

export class A2j {

    state: number = 0;
    totalTime: number = 0;
    runsLeft: number = MS2NS;
    noEqual: string = '==';
    equal: string = '!=';

    public setup(): void {
        console.log('starting');
    }

    bench(): int {
        return 1;
    }

    message(data: string): void {
        console.log('Benchmark result: A2j ' + data);
    }

    test(): number {
        this.setup();
        this.bench();

        // NOTE issue (17741) - enable below after fix import bigInt
        if (false) {
            this.comparison<BigInt>(this.runsLeft, jsBigInt, jsBigInt, this.equal);
        }

        this.comparison<boolean>(this.runsLeft, jsBool, jsBool, this.equal, 'boolean ');

        this.comparison(this.runsLeft, jsNull, jsNull, this.equal, 'null ');

        // NOTE: (19193) enable below after fix comparison
        if (false) {
            this.comparison<undefined | string>(this.runsLeft, jsUndefined, jsUndefined, this.equal, 'undefined ');
        }

        return this.totalTime;
    }

    comparisonObject(iterations: number, valueA: JSValue, valueB: JSValue, compare: string, target: string): void {
        let start: long;
        let loopTime: long = 0;

        while (iterations > 0) {
            start = Chrono.nanoNow();

            if (compare == this.equal && valueA !== valueB) throw Error();
            if (compare == this.noEqual && valueA === valueB) throw Error();

            loopTime += Chrono.nanoNow() - start;
            iterations = iterations - 1;
        }

        this.message(target + loopTime);

        this.totalTime += loopTime;
    }

    comparison<T>(iterations: number, valueA: T, valueB: T, compare: string, target: string): void {
        let start: long;
        let loopTime: long = 0;

        while (iterations > 0) {
            start = Chrono.nanoNow();
            if (compare == this.equal && valueA != valueB) throw Error();
            if (compare == this.noEqual && valueA == valueB) throw Error();

            loopTime += Chrono.nanoNow() - start;
            iterations = iterations - 1;
        }

        this.message(target + loopTime);

        this.totalTime += loopTime;
    }
}
