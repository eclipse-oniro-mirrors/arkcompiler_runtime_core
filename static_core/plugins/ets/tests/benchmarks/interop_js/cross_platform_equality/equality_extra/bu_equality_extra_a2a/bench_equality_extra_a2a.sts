class ClassTest {
    a: int;

    constructor(a: int) {
        this.a = a;
    }
}

const stsArr: int[] = [];
const stsArrCopy: int[] = stsArr;
const stsArrOther: int[] = [];
const stsArrowFunction = () => { };
const stsArrowFunctionCopy = stsArrowFunction;
const stsArrowFunctionOther = () => { };
function foo() { }
function fooOther() { }

const MS2NS = 1000000;

class EqualityExtraA2a {

    state: number = 0;
    totalTime: number = 0;
    runsLeft: number = MS2NS;
    noEqual: string = '==';
    equal: string = '!=';

    public setup(): void {
        console.log('starting');
    }

    bench(): int {
        return 1;
    }

    message(data: string): void {
        console.log('Benchmark result: equality_extra_2a ' + data);
    }

    test(): number {
        this.setup();
        this.bench();

        //NOTE: issue(19194) enable below after fix compare arrays
        if (false) {
            this.comparison(this.runsLeft, stsArr, stsArr, this.equal, 'array ');
            this.comparison(this.runsLeft, stsArr, stsArrCopy, this.equal, 'array by link ');
        }

        this.comparison(this.runsLeft, stsArr, stsArrOther, this.noEqual, 'different arrays ');

        this.comparison(this.runsLeft, stsArrowFunction, stsArrowFunction, this.equal, 'arrow function ');

        this.comparison(this.runsLeft, stsArrowFunction, stsArrowFunctionCopy, this.equal, 'arrow function by link ');

        this.comparison(this.runsLeft, stsArrowFunction, stsArrowFunctionOther, this.noEqual, 'different arrows ');

        //NOTE: issue (19133) enable below after fix comparison functions
        if (false) {
            this.comparison(this.runsLeft, foo, foo, this.equal, 'function ');
            this.comparison(this.runsLeft, foo, fooOther, this.noEqual, 'different functions ');
        }

        return this.totalTime ;
    }

    comparison<T>(iterations: number, valueA: T, valueB: T, compare: string, target: string): void {
        let start: long;
        let loopTime: long = 0;

        while (iterations > 0) {
            start = Chrono.nanoNow();
            if (compare == this.equal && valueA != valueB) throw Error();
            if (compare == this.noEqual && valueA == valueB) throw Error();

            loopTime += Chrono.nanoNow() - start;
            iterations = iterations - 1;
        }

        this.message(target + loopTime);

        this.totalTime += loopTime;
    }
}
