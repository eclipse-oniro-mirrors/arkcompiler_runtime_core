/*---
desc: >-
      A class can implement Optional Interface Properties) from superinterfaces
      or use implicitly defined accessors from an interface.
---*/

interface I {
  n?: number
}

class C implements I {
  n?: number = 42
}

function main() {
  let c = new C()
  arktest.assertEQ(c.n, 42)
  c.n = 43
  arktest.assertEQ(c.n, 43)
}
