/*---
desc: Call default interface method by specifying the type of variable as Interface
---*/

interface I {
  foo(): int {
    return 1
  }
}

abstract class A implements I {
  foo(): int {
    return 2
  }
}

class C extends A {}

function main() {
  let i: I = new C()
  arktest.assertEQ(i.foo(), 2)
}
