/*---
desc: Object is used before being initialized
tags: [no-warmup]
---*/

{{c.decl}}

let res: boolean = false

function main() {
  try {
    A.a
  } catch(e) {
    arktest.assertTrue(e instanceof NullPointerError)
    res = true
  }
  arktest.assertTrue(res)
}
