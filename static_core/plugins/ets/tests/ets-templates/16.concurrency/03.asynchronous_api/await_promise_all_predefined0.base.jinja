{% macro generate_complete_test(cases) %}
{% for c in cases %}
{% for type_name in [c.type, c.alias] | select %}

/*---
desc: Test to wait Promise.all for Promises<{{type_name}}>.
---*/

const value: {{type_name}} = {{c.value}}
const length = 3;

async function promise1(): Promise<{{type_name}}> {
    return value;
}

async function promise2(): Promise<{{type_name}}> {
    return value;
}

async function promise3(): Promise<{{type_name}}> {
    return value;
}

async function fetchMultipleData() {
    return await Promise.all([promise1(),promise2(),promise3()]);
}

async function startTest() {
    const result = await fetchMultipleData();
    arktest.assertTrue(result instanceof Array, "Result type must be Array");
    arktest.assertEQ(result.length, length, `Array of Promises length must be ${length}`);
}

function main() {
    waitForCompletion(startTest);
}
{% endfor %}
{% endfor %}
{% endmacro %}