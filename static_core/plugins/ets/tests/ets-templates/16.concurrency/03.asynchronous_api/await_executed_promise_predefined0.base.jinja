{% macro generate_complete_test(cases) %}
{% for c in cases %}
{% for type_name in [c.type, c.alias] | select %}

/*---
desc: Test to wait for already executed Promise<{{type_name}}>.
---*/

const value: {{type_name}} = {{c.value}}

async function startTest() {
    const resolvedPromise = Promise.resolve(value); 
    const result = await resolvedPromise;
    arktest.assertEQ(result, value, `Promise.resolve result should be ${value}`);
}

function main() {
    waitForCompletion(startTest); 
}
{% endfor %}
{% endfor %}
{% endmacro %}