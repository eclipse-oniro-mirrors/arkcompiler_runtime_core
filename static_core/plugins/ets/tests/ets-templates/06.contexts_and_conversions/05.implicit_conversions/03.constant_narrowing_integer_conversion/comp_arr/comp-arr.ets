{%- for c in cases %}

/*---
desc: Composite context, array element, constant narrowing integer conversion, implicit
params: from {{c.from_type}} to {{c.to_type}}
---*/
{%- for v in c['values'] %}
const c{{loop.index}}: {{c.from_type}} = {{v.expr|safe}}
{%- endfor %}

function main() {
{%- for v in c['values'] %}
    let d{{loop.index}}: {{c.to_type}}[] = [({{v.expr|safe}}).to{{c.to_type}}(), ({{v.expr|safe}}).to{{c.to_type}}()]  // expression
    arktest.assertEQ(d{{loop.index}}[1], ({{v.val}}).to{{c.to_type}}())

    d{{loop.index}} = [c{{loop.index}}.to{{c.to_type}}()]  // constant
    arktest.assertEQ(d{{loop.index}}[0], ({{v.val}}).to{{c.to_type}}())
{%- endfor %}
}
{%- endfor %}
