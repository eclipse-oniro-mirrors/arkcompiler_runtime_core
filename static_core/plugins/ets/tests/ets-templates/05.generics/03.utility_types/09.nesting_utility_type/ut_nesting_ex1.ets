/*---
desc: If more than one utility types are required then they can be nested
tags: [compile-only, negative]
---*/

interface Issue {
    title? string
    description?: string
}

function main() {
    const myIssue: Required<Readonly<Issue>> = { title: 'aa', description: 'description' }
    //  'TypeError: The 'Readonly' property cannot be reassigned.
    myIssue.description = 'Not allowed to change as it readonly';
}
