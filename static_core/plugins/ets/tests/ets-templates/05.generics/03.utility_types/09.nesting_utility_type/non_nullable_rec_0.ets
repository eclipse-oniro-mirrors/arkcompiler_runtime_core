/*---
desc: If more than one utility types are required then they can be nested
tags: [compile-only, negative]
---*/

type NS = String | null
type Rec = Record<number, NonNullable<NS>>


function main() {
    // TypeError: Type 'null' is not compatible with type 'String' at index 2
    let nonNullableValue: Rec = {0: 'Hello', 1: 'World', 3: null}

}


