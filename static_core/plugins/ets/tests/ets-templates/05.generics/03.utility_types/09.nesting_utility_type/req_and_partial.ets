/*---
desc: If more than one utility types are required then they can be nested
tags: [compile-only, negative]
---*/

interface Issue {
    title: string // Required field
    description?: string
}

type AllToOp = Partial<Issue> // Drop all properties to optional
type AllToRec = Required<AllToOp> // Set all properties to required

function main() {
    //  TypeError: Class property 'title' needs to be initialized for Required<Issue>
    let xx: AllToRec = { description: 'description' }
}
