{% for c in cases %}
/*---
desc: >-
    The result of a string indexing expression is a value of string type.
---*/

let str: string = "\x17\x2b\xef\0\u1f48\u2331\u2b03\u2eec \n \u{CAFE}\n\uBABF生日快乐!С Днём рождения!";

function main(): void {
  let index: {{c.type}} = {{c.value}};
  arktest.assertEQ(str[index], '{{c.symbol}}');
  arktest.assertTrue(str[index] instanceof string)
  arktest.assertEQ(typeof str[index], 'string')
}
{% endfor %}
