import { Decimal } from "@arkts.math.Decimal";

function main(): int {
    const suite = new arktest.ArkTestsuite("Decimal add API tests");
    suite.addTest("Decimal add with specific config", testAddWithSpecificConfig001);
    suite.addTest("Decimal add with specific config", testAddWithSpecificConfig002);
    suite.addTest("Decimal add with specific config", testAddWithSpecificConfig003);
    suite.addTest("Decimal add with specific config", testAddWithSpecificConfig004);
    suite.addTest("Decimal add with specific config", testAddWithSpecificConfig005);
    suite.addTest("Decimal add with different precision and rounding", testAddWithDiffPrecisionAndRounding001);
    suite.addTest("Decimal add with different precision and rounding", testAddWithDiffPrecisionAndRounding002);
    suite.addTest("Decimal add with different precision and rounding", testAddWithDiffPrecisionAndRounding003);
    suite.addTest("Decimal add with different precision and rounding", testAddWithDiffPrecisionAndRounding004);
    suite.addTest("Decimal add with different precision and rounding", testAddWithDiffPrecisionAndRounding005);
    suite.addTest("Decimal add with different precision and rounding", testAddWithExpNegAndExpPosIsZero001);
    suite.addTest("Decimal add with different precision and rounding", testAddWithExpNegAndExpPosIsZero002);
    suite.addTest("Decimal add with different precision and rounding", testAddWithExpNegAndExpPosIsZero003);
    return suite.run();
}

function checkAddResult(addendA: string, addendB: string, expected: string, sd: double, rm: int) {
    Decimal.precision = sd;
    Decimal.rounding = rm;
    arktest.assertEQ(new Decimal(addendA).add(addendB).valueOf(), expected);
}

function testAddWithSpecificConfig001() {
    Decimal.set({ precision: 200, rounding: 4, toExpNeg: -9e9, toExpPos: 9e9, minE: -9e15, maxE: 9e15 });
    arktest.assertEQ(new Decimal(1).add(0).valueOf(), '1');
    arktest.assertEQ(new Decimal(1).add(-0).valueOf(), '1');
    arktest.assertEQ(new Decimal(-1).add(0).valueOf(), '-1');
    arktest.assertEQ(new Decimal(-1).add(-0).valueOf(), '-1');
    arktest.assertEQ(new Decimal(1).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-1).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(1).add('Infinity').valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(1).add('-Infinity').valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-1).add('Infinity').valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(-1).add('-Infinity').valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(0).add(1).valueOf(), '1');
    arktest.assertEQ(new Decimal(0).add(-1).valueOf(), '-1');
    arktest.assertEQ(new Decimal(-0).add(1).valueOf(), '1');
    arktest.assertEQ(new Decimal(-0).add(-1).valueOf(), '-1');
}

function testAddWithSpecificConfig002() {
    Decimal.set({ precision: 200, rounding: 3, toExpNeg: -9e9, toExpPos: 9e9, minE: -9e15, maxE: 9e15 });
    arktest.assertEQ(new Decimal('0').add('0').valueOf(), '0');
    arktest.assertEQ(new Decimal('-0').add('-0').valueOf(), '-0');
}

function testAddWithSpecificConfig003() {
    Decimal.set({ precision: 200, rounding: 4, toExpNeg: -9e9, toExpPos: 9e9, minE: -9e15, maxE: 9e15 });
    arktest.assertEQ(new Decimal('0').add('-0').valueOf(), '0');
    arktest.assertEQ(new Decimal('-0').add('0').valueOf(), '0');
    arktest.assertEQ(new Decimal('0').add('0').valueOf(), '0');
    arktest.assertEQ(new Decimal('-0').add('-0').valueOf(), '-0');
    arktest.assertEQ(new Decimal('1').add('-1').valueOf(), '0');
    arktest.assertEQ(new Decimal('-1').add('1').valueOf(), '0');
    arktest.assertEQ(new Decimal(0).add(NaN).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-0).add(NaN).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(0).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(0).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-0).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(-0).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(1).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-1).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(0).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-0).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal('NaN').add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(Infinity).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-Infinity).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(1).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(-1).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(1).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-1).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(0).add(Infinity).valueOf(), 'Infinity');
}

function testAddWithSpecificConfig004() {
    arktest.assertEQ(new Decimal(-0).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(0).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-0).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(Infinity).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-Infinity).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(Infinity).add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(Infinity).add(-Infinity).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-Infinity).add(Infinity).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-Infinity).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(1).add('0').valueOf(), '1');
    arktest.assertEQ(new Decimal(1).add('1').valueOf(), '2');
    arktest.assertEQ(new Decimal(1).add('-45').valueOf(), '-44');
    arktest.assertEQ(new Decimal(1).add('22').valueOf(), '23');
    arktest.assertEQ(new Decimal(1).add(0o144).valueOf(), '101');
    arktest.assertEQ(new Decimal(1).add('0144').valueOf(), '145');
    arktest.assertEQ(new Decimal(1).add('6.1915').valueOf(), '7.1915');
    arktest.assertEQ(new Decimal(1).add('-1.02').valueOf(), '-0.02');
    arktest.assertEQ(new Decimal(1).add('0.09').valueOf(), '1.09');
    arktest.assertEQ(new Decimal(1).add('-0.0001').valueOf(), '0.9999');
    arktest.assertEQ(new Decimal(1).add('8e5').valueOf(), '800001');
    arktest.assertEQ(new Decimal(1).add('9E12').valueOf(), '9000000000001');
    arktest.assertEQ(new Decimal(1).add('1e-14').valueOf(), '1.00000000000001');
    arktest.assertEQ(new Decimal(1).add('3.345E-9').valueOf(), '1.000000003345');
    arktest.assertEQ(new Decimal(1).add('-345.43e+4').valueOf(), '-3454299');
    arktest.assertEQ(new Decimal(1).add('-94.12E+0').valueOf(), '-93.12');
    arktest.assertEQ(new Decimal('0').add(0).valueOf(), '0');
    arktest.assertEQ(new Decimal('0').add('0').valueOf(), '0');
    arktest.assertEQ(new Decimal(3).add(-0).valueOf(), '3');
    arktest.assertEQ(new Decimal(9.654).add(0).valueOf(), '9.654');
    arktest.assertEQ(new Decimal(0).add('0.001').valueOf(), '0.001');
}

function testAddWithSpecificConfig005() {
    arktest.assertEQ(new Decimal(0).add('111.1111111110000').valueOf(), '111.111111111');
    arktest.assertEQ(new Decimal('NaN').add('0').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(-1).add(1).valueOf(), '0');
    arktest.assertEQ(new Decimal(-0.01).add(0.01).valueOf(), '0');
    arktest.assertEQ(new Decimal(54).add(-54).valueOf(), '0');
    arktest.assertEQ(new Decimal(9.99).add('-9.99').valueOf(), '0');
    arktest.assertEQ(new Decimal('0.00000').add('-0.000001').valueOf(), '-0.000001');
    arktest.assertEQ(new Decimal('0.0000023432495704937').add('-0.0000023432495704937').valueOf(), '0');
    arktest.assertEQ(new Decimal(NaN).add(NaN).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(NaN).add('NaN').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal('NaN').add(NaN).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal('NaN').add(4).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal('NaN').add('4534534.45435435').valueOf(), 'NaN');
    arktest.assertEQ(new Decimal('NaN').add(99999.999).valueOf(), 'NaN');
    arktest.assertEQ(new Decimal(Infinity).add('354.345341').valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(3).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-Infinity).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(-Infinity).add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(Infinity).add('-999e999').valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal('1.21123e43').add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal('-999.0').add(Infinity).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal('657.342e-45').add(-Infinity).valueOf(), '-Infinity');
    arktest.assertEQ(new Decimal(Infinity).add(123).valueOf(), 'Infinity');
    arktest.assertEQ(new Decimal(100).add(100).valueOf(), '200');
    arktest.assertEQ(new Decimal(-999.99).add('0.01').valueOf(), '-999.98');
    arktest.assertEQ(new Decimal('10').add(4).valueOf(), '14');
    arktest.assertEQ(new Decimal('03.333').add(-4).valueOf(), '-0.667');
    arktest.assertEQ(new Decimal(-1).add(-0.1).valueOf(), '-1.1');
    arktest.assertEQ(new Decimal(43534.5435).add('0.054645').valueOf(), '43534.598145');
    arktest.assertEQ(new Decimal('99999').add('1').valueOf(), '100000');
}

