#! CHECKER      Static Any Ldbyname and Stbyname INT
#! RUN          options: "--compiler-enable-jit=false", entry: "_GLOBAL::main"

#! CHECKER      Static Any Ldbyname and Stbyname JIT
#! RUN          force_jit: true, options: "--compiler-regex='.*test.*byname.*'", entry: "_GLOBAL::main"
#! METHOD       "_GLOBAL::testLdbyname"
#! PASS_AFTER   "IrBuilder"
#! INST         /Intrinsic.CompilerAnyLdbyname/
#! METHOD       "_GLOBAL::testStbyname"
#! PASS_AFTER   "IrBuilder"
#! INST         /Intrinsic.CompilerAnyStbyname/

#! CHECKER      Static Any Ldbyname and Stbyname AOT
#! RUN_PAOC     options: "--compiler-regex='.*test.*byname.*'"
#! METHOD       "_GLOBAL::testLdbyname"
#! PASS_AFTER   "IrBuilder"
#! INST         /Intrinsic.CompilerAnyLdbyname/
#! METHOD       "_GLOBAL::testStbyname"
#! PASS_AFTER   "IrBuilder"
#! INST         /Intrinsic.CompilerAnyStbyname/
#! RUN          entry: "_GLOBAL::main"

.language eTS

.record std.core.Int <external>

.record std.core.Object <external>

.record R <ets.extends=std.core.Object, access.record=public> {
    i32 a
}

.function void R._ctor_(R a0) <ctor, access.function=public> {
    call.short std.core.Object._ctor_:(std.core.Object), a0
    ldai 42
    stobj a0, R.a
    return.void
}

.function i32 testLdbyname() {
    initobj.short R._ctor_:(R)
    sta.obj v0
    any.ldbyname v0, "a"
    sta.obj v2
    movi v1, 42
    initobj.short std.core.Int._ctor_:(std.core.Int,i32), v1
    sta.obj v1
    ets.equals v1, v2
    return
}

.function i32 testStbyname() {
    initobj.short R._ctor_:(R)
    sta.obj v0
    movi v1, 64
    initobj.short std.core.Int._ctor_:(std.core.Int,i32), v1
    any.stbyname v0, "a"
    ldobj v0, R.a
    ucmp v1
    return
}

.function i32 main() <static, access.function=public> {
    call.short testLdbyname
    jeqz exit_ld
    call.short testStbyname
    jnez exit_st
    ldai 0
    return
exit_ld:
    ldai -1
    return
exit_st:
    ldai -2
    return
}

.function void std.core.Int._ctor_(std.core.Int a0, i32 a1) <ctor, external, access.function=public>

.function void std.core.Object._ctor_(std.core.Object a0) <ctor, external, access.function=public>
