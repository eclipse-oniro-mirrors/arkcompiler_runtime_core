interface S1 { }
interface S2 { }
interface S3 extends S1, S2 { }
class CS3 implements S3 { }

interface I {
    foo(a: S3) { return "I" }
}

class A implements I {
    foo(a: S1) { return "A" }
}

class B extends A {
    foo(a: S1) { return "B1" }
    foo(a: S2) { return "B2" }
}

arktest.assertEQ(
    ((i: A) => i.foo(new CS3()))(new B()),
    "B1"
)
