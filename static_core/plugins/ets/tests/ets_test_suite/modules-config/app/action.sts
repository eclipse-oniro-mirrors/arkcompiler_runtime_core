import * as foo from "@foo/thing"

function testNames() {
    const nameOf = (o: Object) => Class.of(o).getName()

    assert(Class.ofCaller().getName() == "@app.action.ETSGLOBAL")

    let kitty = new foo.Kitty()
    assert(nameOf(kitty) == "@foo.thing.Kitty")
    assert(nameOf(kitty.caught()) == "@foo.some.Birdie")
}

testNames()
