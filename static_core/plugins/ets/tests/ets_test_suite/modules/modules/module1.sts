import * as foo from "../foo/foo"
import * as bar from "../bar/bar"
import * as module2 from "./module2"
import * as module3 from "./module3"

export class Dummy { }

export function dummy(): Dummy { return new Dummy() }

function testNames() {
    const nameOf = (o: Object) => Class.of(o).getName()

    assert(bar.dummy() instanceof bar.Dummy);
    assert(nameOf(bar.dummy()) == "pkg.bar.Dummy")

    assert(foo.dummy() instanceof foo.Dummy);
    assert(nameOf(foo.dummy()) == "pkg.foo.Dummy")

    assert(dummy() instanceof Dummy);
    assert(nameOf(dummy()) == "modules.module1.Dummy")

    assert(module2.dummy() instanceof module2.Dummy);
    assert(nameOf(module2.dummy()) == "modules.module2.Dummy")

    assert(module3.dummy() instanceof module3.Dummy);
    assert(nameOf(module3.dummy()) == "modules.module3.Dummy")
}

testNames()
