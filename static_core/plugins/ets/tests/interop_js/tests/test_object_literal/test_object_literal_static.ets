import { Foo, FooInter } from "testDecl";

function testFoo() {
    let foo : Foo = {
        bar: 111,
        baz: "object literal"
    };
    arktest.assertEQ(foo.bar, 111);
    arktest.assertEQ(foo.baz, "object literal");
    return foo;
}

function testFooInter() {
    let foo : FooInter = {
        bar: 222,
        baz: "interface"
    };
    arktest.assertEQ(foo.bar, 222);
    arktest.assertEQ(foo.baz, "interface");
    return foo;
}
