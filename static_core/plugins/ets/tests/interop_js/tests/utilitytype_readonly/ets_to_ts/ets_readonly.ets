class Test {
	public name: string = '';
	public data1: number = 0;
	public data2: number = 0;

	constructor(name: string, data1: number, data2: number) {
		this.name = name;
		this.data1 = data1;
		this.data2 = data2;
	}
}

type ReadonlyTypeTest = Readonly<Test>;
type WritableTypeTest = Test;

let readonlyTestInEts: ReadonlyTypeTest = new Test('readonly', 1, 2);
let writableTestInEts: WritableTypeTest = new Test('writable', 3, 4);

function checkReadonlyTypeDataInEts(data: ReadonlyTypeTest): number {
	return data.data1 + data.data2;
}

function checkWritableTypeDataInEts(data: WritableTypeTest): number {
	return data.data1 + data.data2;
}
