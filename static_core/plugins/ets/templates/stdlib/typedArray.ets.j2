        let cmp = (l: {{T}}, r: {{T}}): int => {
            const result = compareFn!({{asElementCompat % 'l'}}, {{asElementCompat % 'r'}})
            {%- if N == 'BigInt64' %}
            if (result instanceof BigInt) {
