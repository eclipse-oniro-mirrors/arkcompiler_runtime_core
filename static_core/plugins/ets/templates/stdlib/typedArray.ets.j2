        if (this.lengthInt > MAX_SHORT_LENGTH) {
            let arr: FixedArray<{{T}}> = new FixedArray<{{T}}>(this.lengthInt)
            for (let i = 0; i < this.lengthInt; ++i) {
            {%- if N in ['Float32', 'Float64'] %}
                let val = this.getUnsafe(i)
                if (isNaN(val)) {
                    naNCounter++
