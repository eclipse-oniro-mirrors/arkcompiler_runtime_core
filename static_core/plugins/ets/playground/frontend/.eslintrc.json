{
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": ["error"],
      "react-hooks/exhaustive-deps": "off",
      "no-useless-escape": "off"
    }
}
