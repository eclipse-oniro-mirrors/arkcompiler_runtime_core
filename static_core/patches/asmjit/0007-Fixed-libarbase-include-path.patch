From 00698eed49653df26eda74b37f6d5727d2f58663 Mon Sep 17 00:00:00 2001
From: ivagin <>
Date: Mon, 23 Jun 2025 21:58:59 +0800
Subject: [PATCH] Moved static_core/libpandabase->libarkbase

---
 BUILD.gn                     | 6 +++---
 src/asmjit/core/codeholder.h | 2 +-
 src/asmjit/core/zone.h       | 2 +-
 3 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/BUILD.gn b/BUILD.gn
index c324757..f7b4448 100644
--- a/BUILD.gn
+++ b/BUILD.gn
@@ -151,20 +151,20 @@ asmjit_sources = [
 asmjit_configs = [
     ":asmjit_public_config",
     "$ark_root:ark_config",
-    "$ark_root/libpandabase:arkbase_public_config",
+    "$ark_root/libarkbase:arkbase_public_config",
     ":asmjit_config",
 ]
 
 ohos_static_library("libasmjit") {
     sources = asmjit_sources
     configs = asmjit_configs
-    deps = [ "$ark_root/libpandabase:libarktsbase" ]
+    deps = [ "$ark_root/libarkbase:libarktsbase" ]
     subsystem_name = "ark_subsystem_name"
 }
 
 ohos_static_library("libasmjit_frontend_static") {
     sources = asmjit_sources
     configs = asmjit_configs
-    deps = [ "$ark_root/libpandabase:libarktsbase_frontend_static" ]
+    deps = [ "$ark_root/libarkbase:libarktsbase_frontend_static" ]
     subsystem_name = "ark_subsystem_name"
 }
diff --git a/src/asmjit/core/codeholder.h b/src/asmjit/core/codeholder.h
index 81102a5..784ddcd 100644
--- a/src/asmjit/core/codeholder.h
+++ b/src/asmjit/core/codeholder.h
@@ -38,7 +38,7 @@
 #include "../core/zonetree.h"
 #include "../core/zonevector.h"
 
-#include "mem/arena_allocator.h"
+#include "libarkbase/mem/arena_allocator.h"
 
 ASMJIT_BEGIN_NAMESPACE
 
diff --git a/src/asmjit/core/zone.h b/src/asmjit/core/zone.h
index 04b2f7b..2110813 100644
--- a/src/asmjit/core/zone.h
+++ b/src/asmjit/core/zone.h
@@ -26,7 +26,7 @@
 
 #include "../core/support.h"
 
-#include "mem/arena_allocator.h"
+#include "libarkbase/mem/arena_allocator.h"
 
 ASMJIT_BEGIN_NAMESPACE
 
-- 
2.42.0-openssl

