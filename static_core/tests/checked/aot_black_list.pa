#! CHECKER      All methods compiled
#! RUN_PAOC     options: "--compiler-inlining false"
#! RUN          options: "", entry: "_GLOBAL::main", result: 58
#! EVENT        "AotEntrypointFound,_GLOBAL::func1"
#! EVENT        "AotEntrypointFound,_GLOBAL::func2"
#! EVENT        "AotEntrypointFound,_GLOBAL::func3"
#! EVENT        "AotEntrypointFound,_GLOBAL::main"
#! EVENT        "AotEntrypointFound,Test::f1"
#! EVENT        "AotEntrypointFound,Test::f2"
#! EVENT        "AotEntrypointFound,Test::f3"

#! CHECKER      Methods in black list not compiled
#! WRITE_FILE   inputs: "Test::f1\nTest::f2\nTest::f3", outputs: "black_list_methods", ext: "txt"
#! RUN_PAOC     options: "--compiler-inlining=false --paoc-methods-from-file:iswhite=false --paoc-methods-from-file:path='black_list_methods.txt'"
#! RUN          options: "", entry: "_GLOBAL::main", result: 58
#! EVENT        "AotEntrypointFound,_GLOBAL::func1"
#! EVENT        "AotEntrypointFound,_GLOBAL::func2"
#! EVENT        "AotEntrypointFound,_GLOBAL::func3"
#! EVENT        "AotEntrypointFound,_GLOBAL::main"
#! EVENT_NOT    "AotEntrypointFound,Test::f1"
#! EVENT_NOT    "AotEntrypointFound,Test::f2"
#! EVENT_NOT    "AotEntrypointFound,Test::f3"

#! CHECKER      Methods in white list compiled
#! WRITE_FILE   inputs: "Test::f1\nTest::f2\nTest::f3", outputs: "white_list_methods", ext: "txt"
#! RUN_PAOC     options: "--compiler-inlining=false --paoc-methods-from-file:path='white_list_methods.txt'"
#! RUN          options: "", entry: "_GLOBAL::main", result: 58
#! EVENT_NOT    "AotEntrypointFound,_GLOBAL::func1"
#! EVENT_NOT    "AotEntrypointFound,_GLOBAL::func2"
#! EVENT_NOT    "AotEntrypointFound,_GLOBAL::func3"
#! EVENT_NOT    "AotEntrypointFound,_GLOBAL::main"
#! EVENT        "AotEntrypointFound,Test::f1"
#! EVENT        "AotEntrypointFound,Test::f2"
#! EVENT        "AotEntrypointFound,Test::f3"

.record Test {}

.function i32 Test.f1() {
    ldai 1
    return
}

.function i32 Test.f2() {
    ldai 2
    return
}

.function i32 Test.f3() {
    ldai 3
    return
}

.function i32 func1(i32 a0) {
    ldai 2
    add2 a0
    return
}

.function i32 func2(i32 a0) {
    ldai 2
    mul2 a0
    return
}

.function i32 func3(i32 a0) {
    call.short func1, a0
    sta a0
    call.short func2, a0
    return
}

.function i32 main() {
    movi v1, 10

    call.short func1, v1
    sta v1
    call.short func2, v1
    sta v1
    call.short func3, v1
    sta v1

    call.short Test.f1
    add2 v1
    sta v1

    call.short Test.f2
    add2 v1
    sta v1

    call.short Test.f3
    add2 v1
    sta v1

    return
}

