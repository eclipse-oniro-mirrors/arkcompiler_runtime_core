function task_1(i: int, str: string): string {
    return str + "_" + i;
}

function foo(): string {
    const N = 10;
    let promises = new Array<Promise<Object|undefined|null>>();
    for (let j = 0; j < N; ++j) {
        let p = taskpool.execute(task_1, j, "str");
        promises.push(p);
    }
    try {
        GC.waitForFinishGC(GC.startGC(GC.YOUNG_CAUSE));
    } catch(e) {
        console.log(e)
    }
    let res = "";
    for (let j = 0; j < N; ++j) {
        res += (await promises[j]) + "\n";
    }
    return res;
}