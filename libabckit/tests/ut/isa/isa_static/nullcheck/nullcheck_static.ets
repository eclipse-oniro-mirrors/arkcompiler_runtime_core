class A {}

function foo() : A|null {
  let a = new A();
  // AOP add ets.nullcheck(a|nullptr) here
  // for nullptr should throw NPE
  return a;
}



function main() {
  try {
    let b: A|null = foo();
    console.log(b);
  } catch(e) {
    console.log(e);
  }
}
